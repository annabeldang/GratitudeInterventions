# round duration statistics
mutate(`mean duration (mins)` =
round(x = `mean duration (mins)`,
digits = 2),
`median duration (mins)` =
round(x = `median duration (mins)`,
digits = 2)) %>%
# add date
mutate(`Last updated` = Sys.Date())
